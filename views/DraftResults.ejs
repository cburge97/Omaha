	<style>
		body{
            font-family: Verdana;
            color: black;
			background-color: white;
		}
		.bg-color{
			background-image: linear-gradient(to right,#B4975A, black);;
		}

		th{
		background-color: #333F42;
		color: azure;
		}
		
		.my-custom-scrollbar {
			position: relative;
			height: 250px;
			overflow: auto;
		}
		.my-custom-scrollbar2 {
			position: relative;
			height: 300px;
			overflow: auto;
		}
