<div style="padding-left: 1.25cm; padding-top: 1cm; padding-right: 1.25cm">
<h1>Draft</h1>
<form action="Draft">
<div id="feedback">
	<h4>Feedback:</h4>
	<div class="table-wrapper-scroll-y my-custom-scrollbar" style="padding-bottom: 1.5cm">

		<table class="table table-bordered table-striped mb-0">
	  <thead>
	  </thead>
	  <tbody id="myTable">
			  <% message.forEach((message) => { %>
				  <tr>
					<td ><%= message %></td>
				 </tr>
			 <% }) %>
  </tbody>
	</table>

</div>

	
</div>
<button type="submit" class="btn btn-primary submit">Draft Again</button> 
</form>
</body>
	
	<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
